class Solution {
    public String reverseWords(String s) {
        String[] str=s.split(" ");
String stri="";
for(int i=0;i<str.length;i++){
StringBuilder sb=new StringBuilder (str[i]);
if(i==str.length-1)
stri+=sb.reverse().toString();
else
stri+=sb.reverse().toString() +" ";
}
return stri;
    }
}
