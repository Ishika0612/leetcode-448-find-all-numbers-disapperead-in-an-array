class Solution {
    public char nextGreatestLetter(char[] letters, char target) {
        int a=0;
        Arrays.sort(letters);
        for(int i=0;i<letters.length;i++){
            if(letters[i]<=target)
            continue;
            else{
           a=i;
           break;
            }
        }
        return letters[a];
    }
}
